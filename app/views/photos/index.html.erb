<div class="col-md-6">
  <div class="h-100 p-5 text-white bg-dark rounded-3">
<h1>Мои фотографии</h1>

<div id="photos">
  <% @photos.each do |photo| %>
    <%= render photo %>
    <p>
      <%= link_to "Посмотреть фотографию", photo, class: "btn btn-primary" %>
    </p>
  <% end %>
</div>

    <% if user_signed_in? %>
<%= link_to "Добавить фотографию", new_photo_path, class: "btn btn-primary" %>
<% end %>

</div>
</div>